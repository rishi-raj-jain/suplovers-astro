---
import { getEntry } from "astro:content";
import ClassesLayout from "../pageLayouts/ClassesLayout.astro";
const { data: masterClass } = await getEntry("classes", "es/master-class");
const { data: miniClass } = await getEntry("classes", "es/mini-class");
const { data: advancedClass } = await getEntry("classes", "es/avanzada");
---

<ClassesLayout
  masterClass={masterClass}
  miniClass={miniClass}
  advancedClass={advancedClass}
/>
