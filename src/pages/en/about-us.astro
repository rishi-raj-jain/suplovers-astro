---
import { getEntry } from "astro:content";
import AboutUsLayOut from "../../pageLayouts/AboutUsLayout.astro";
const {
  data: { about_us, philosophy, join_us, headMetaData },
} = await getEntry("about_us", "en");
---

<AboutUsLayOut
  headMetaData={headMetaData}
  about_us={about_us}
  philosophy={philosophy}
  join_us={join_us}
/>
